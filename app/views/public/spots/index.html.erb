<h1>Public::Spots#index</h1>
<p>Find me in app/views/public/spots/index.html.erb</p>
<%= form_tag({controller:"spots",action:"index"}, method: :get) do %>
    <% Tag.all.each do |t| %>
        <li><%= check_box :tag_ids, t.name %><%= t.name %></li>
    <% end %>
    <%= submit_tag '検索' %>
<% end %>

<% @spots.each do |spot| %>
<%= spot.name %>
<% end %>
